30 0,6,12,18 * * * PLATFORM=SunOS-5.6 && . $HOME/b8t/hosts/$PLATFORM/env && make PLATFORM=$PLATFORM -e -f $HOME/b8t/bin/b8t.mk  > $HOME/b8t/hosts/$PLATFORM/b8t-status 2>&1
